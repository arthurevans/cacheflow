    <groundspeak:cache id="1263637" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>River</groundspeak:name>
      <groundspeak:placed_by>X</groundspeak:placed_by>
