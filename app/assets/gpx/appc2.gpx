      <groundspeak:name>T in the Road</groundspeak:name>
      <groundspeak:placed_by>Diana, Jim and Bryan</groundspeak:placed_by>
      <groundspeak:owner id="27228">dmg</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
