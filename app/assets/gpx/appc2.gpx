    <groundspeak:cache id="2567939" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Scary Places (KPC2011)</groundspeak:name>
      <groundspeak:placed_by>kablooey's 2011 Puzzle Challenge</groundspeak:placed_by>
      <groundspeak:owner id="18898">kablooey</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
