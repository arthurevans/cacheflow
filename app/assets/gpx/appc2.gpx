        <groundspeak:log id="251840464">
          <groundspeak:date>2012-07-25T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="751819">Bob&amp;Elliecat</groundspeak:finder>
          <groundspeak:text encoded="False">Did not see any Black Squirrel, but liked your hide very much. Clever.    TFTCTNSLLN</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="249698537">
