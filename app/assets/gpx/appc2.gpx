title="" alt="logo" style=
"border: 0px solid ; width: 57px; height: 89px;" /&gt;&lt;/div&gt;&lt;p&gt;Additional Waypoints&lt;/p&gt;</groundspeak:long_description>
      <groundspeak:encoded_hints>
