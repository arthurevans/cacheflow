          <groundspeak:text encoded="False">Easy find.  No one around in this nice quiet neighborhood.  TNLNSL.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="244067710">
          <groundspeak:date>2012-06-23T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="223494">sleepyrobin</groundspeak:finder>
          <groundspeak:text encoded="True">[I did return this the way I found it, but it's clearly not where it's supposed to be.]  No container, just the plastic bag with the log sheet in it, lying on the ground.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="243733113">
          <groundspeak:date>2012-06-19T22:00:29Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="5984824">Brassinc</groundspeak:finder>
          <groundspeak:text encoded="False">Michael found this one. </groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="228949066">
          <groundspeak:date>2012-04-23T19:00:00Z</groundspeak:date>
