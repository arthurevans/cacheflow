    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=8d057ef5-1685-4141-81dc-91bc8b1c2144</url>
    <urlname>Northern CA Solar System Model: Shoemaker-Levy</urlname>
    <sym>Geocache</sym>
    <type>Geocache|Unknown Cache</type>
    <groundspeak:cache id="62551" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Northern CA Solar System Model: Shoemaker-Levy</groundspeak:name>
      <groundspeak:placed_by>bthomas (Maintained by kindredlaugh)</groundspeak:placed_by>
      <groundspeak:owner id="640198">kindredlaugh</groundspeak:owner>
