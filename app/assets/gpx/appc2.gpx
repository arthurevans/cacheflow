      <groundspeak:name>Driftwood Revisited</groundspeak:name>
      <groundspeak:placed_by>SeanStafford</groundspeak:placed_by>
      <groundspeak:owner id="2306419">SeanStafford&amp;GCWidow</groundspeak:owner>
