    <groundspeak:cache id="830558" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Where The Sidewalk Ends</groundspeak:name>
      <groundspeak:placed_by>Somethings Fishy</groundspeak:placed_by>
      <groundspeak:owner id="347329">Somethings Fishy</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
