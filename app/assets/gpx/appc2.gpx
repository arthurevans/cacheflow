      <groundspeak:short_description html="False">Played soccer here once with my cousin</groundspeak:short_description>
      <groundspeak:long_description html="False">this was made for the senior center</groundspeak:long_description>
      <groundspeak:encoded_hints>fence behind goal</groundspeak:encoded_hints>
      <groundspeak:logs>
        <groundspeak:log id="247321074">
          <groundspeak:date>2012-07-07T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="87748">DavidT21 &amp; Fisherwoman</groundspeak:finder>
          <groundspeak:text encoded="False">When we arrived at GZ we realized we had already found this cache. We checked the log and yes we had found this cache back in May.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="249269670">
          <groundspeak:date>2012-06-17T19:00:00Z</groundspeak:date>
