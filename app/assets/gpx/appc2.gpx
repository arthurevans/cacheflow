          <groundspeak:finder id="190624">Ma &amp; Pa</groundspeak:finder>
          <groundspeak:text encoded="False">Spent the day walking around the area picking up caches and shrugging in frustration.

TFTC</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="225917557">
          <groundspeak:date>2012-04-10T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="1915916">sv_ted</groundspeak:finder>
