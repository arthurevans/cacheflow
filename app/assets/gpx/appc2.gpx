    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="421083" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Jdsinger’s Fairoaks Park</groundspeak:name>
      <groundspeak:placed_by>jdsinger</groundspeak:placed_by>
