    <groundspeak:cache id="1427587" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>1942</groundspeak:name>
      <groundspeak:placed_by>streetwalker1</groundspeak:placed_by>
      <groundspeak:owner id="1943542">streetwalker1</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
