          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="1024537">FF &amp; PC</groundspeak:finder>
          <groundspeak:text encoded="False">Had time to kill while friend had Dr visit at the VA. tftc</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="232666191">
          <groundspeak:date>2012-05-08T03:18:42Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="4506948">Puffpaff</groundspeak:finder>
          <groundspeak:text encoded="False">Up from Oregon visiting my uncle in the VA nice to get out and get some air. TFTC like to dedicate this find to my uncle john.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="232665942">
          <groundspeak:date>2012-05-08T03:15:35Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="4548703">Cpuffy</groundspeak:finder>
          <groundspeak:text encoded="False">Was looking right at it then my brother found it!</groundspeak:text>
