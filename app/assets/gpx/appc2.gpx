          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="1024537">FF &amp; PC</groundspeak:finder>
          <groundspeak:text encoded="False">While waiting for friend who had VA appt., I was able to get in a couple of miles hiking to nearby caches. Lucked out on this hide as I went right to it. TFTC</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="237063721">
          <groundspeak:date>2012-05-26T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="4842721">imoseyon</groundspeak:finder>
          <groundspeak:text encoded="False">Out geocaching with the labios at Stanford. Tftc!</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="236934264">
          <groundspeak:date>2012-05-19T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="276837">justbob</groundspeak:finder>
          <groundspeak:text encoded="False">Another find for me in Stanford Industrial Park, where I worked for 24 years.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="216560239">
          <groundspeak:date>2012-03-04T20:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="578646">rwclady</groundspeak:finder>
          <groundspeak:text encoded="False">I came back today, a Sunday, and found the cache easily.  SL.  TFTC.</groundspeak:text>
        </groundspeak:log>
