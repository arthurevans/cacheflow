    <name>GC39X73</name>
    <desc>Disheveled Ice Heroes (KPC2012) by kablooey's 2012 Puzzle Challenge, Unknown Cache (4.5/1.5)</desc>
    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=fdad3ed4-432e-433d-8baf-71aae9205513</url>
    <urlname>Disheveled Ice Heroes (KPC2012)</urlname>
    <sym>Geocache</sym>
    <type>Geocache|Unknown Cache</type>
    <groundspeak:cache id="2654690" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Disheveled Ice Heroes (KPC2012)</groundspeak:name>
      <groundspeak:placed_by>kablooey's 2012 Puzzle Challenge</groundspeak:placed_by>
      <groundspeak:owner id="18898">kablooey</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
      <groundspeak:difficulty>4.5</groundspeak:difficulty>
      <groundspeak:terrain>1.5</groundspeak:terrain>
      <groundspeak:country>United States</groundspeak:country>
      <groundspeak:state>California</groundspeak:state>
      <groundspeak:short_description html="True">&lt;b&gt;&lt;font color="red"&gt;The cache is not at the above coordinates. Solve the following puzzle to determine the actual coordinates.&lt;/font&gt;&lt;/b&gt;
</groundspeak:short_description>
      <groundspeak:long_description html="True">I was trying to figure out how to honor 1/18/61, but I couldn't seem to fit him into the puzzle. Here are some others he might have shared ice time with:&lt;br /&gt;
&lt;table bgcolor="yellow" cellspacing="2" border="1"&gt;
&lt;tr&gt;
