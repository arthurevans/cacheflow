    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="2096063" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>CDC 7600</groundspeak:name>
      <groundspeak:placed_by>rragan</groundspeak:placed_by>
      <groundspeak:owner id="78712">rragan</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
