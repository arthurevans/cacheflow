        <groundspeak:log id="251898382">
          <groundspeak:date>2012-07-24T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="2074604">CarlottaP</groundspeak:finder>
          <groundspeak:text encoded="False">I found this April 29 (after the Spring Fling Event) with Oaknest and just realized I never logged it. So, I'm logging it now.  Thanks for the fun cache.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="249358620">
