    <desc>Paper Daughter 1926 by Graihwing, Traditional Cache (2/2.5)</desc>
    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=12b53762-677a-40c8-860e-0c6f5ae49d9a</url>
    <urlname>Paper Daughter 1926</urlname>
    <sym>Geocache</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="2577443" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Paper Daughter 1926</groundspeak:name>
      <groundspeak:placed_by>Graihwing</groundspeak:placed_by>
      <groundspeak:owner id="5070830">Graihwing</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
