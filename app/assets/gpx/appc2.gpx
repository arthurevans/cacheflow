    <groundspeak:cache id="437463" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>10 Essentials</groundspeak:name>
      <groundspeak:placed_by>workerofwood, with thanks to Vaudesir</groundspeak:placed_by>
      <groundspeak:owner id="186531">workerofwood</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Regular</groundspeak:container>
