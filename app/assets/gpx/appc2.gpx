    <type>Geocache|Unknown Cache</type>
    <groundspeak:cache id="2985664" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Signs of the Times</groundspeak:name>
      <groundspeak:placed_by>Monsterblader</groundspeak:placed_by>
