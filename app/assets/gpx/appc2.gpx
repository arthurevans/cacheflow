      <groundspeak:name>Sigitem Smörgåsbord Reborn</groundspeak:name>
      <groundspeak:placed_by>xmts (managed by OneAngryPuppy)</groundspeak:placed_by>
      <groundspeak:owner id="376914">oneangrypuppy</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Regular</groundspeak:container>
      <groundspeak:difficulty>2</groundspeak:difficulty>
