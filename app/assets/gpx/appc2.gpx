    <groundspeak:cache id="1780677" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Pickup Lines II: Electric Boogaloo</groundspeak:name>
      <groundspeak:placed_by>xmts (maintained by mokelly)</groundspeak:placed_by>
      <groundspeak:owner id="2981654">mokelly</groundspeak:owner>
