    <groundspeak:cache id="847342" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Blink and You'll Miss It</groundspeak:name>
      <groundspeak:placed_by>GoGoJo</groundspeak:placed_by>
      <groundspeak:owner id="978032">GoGoJo</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
