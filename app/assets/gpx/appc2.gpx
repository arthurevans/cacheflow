    <groundspeak:cache id="717707" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Journey to the Center of...</groundspeak:name>
      <groundspeak:placed_by>E.caballus</groundspeak:placed_by>
      <groundspeak:owner id="1118393">E.caballus</groundspeak:owner>
