          <groundspeak:finder id="2999428">thelins</groundspeak:finder>
          <groundspeak:text encoded="False">Hmm... There seem to be some issues. Will have to take a closer look.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="253739741">
