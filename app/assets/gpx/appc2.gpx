    <groundspeak:cache id="187923" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Free WiFi #2</groundspeak:name>
      <groundspeak:placed_by>WeBeDnD</groundspeak:placed_by>
