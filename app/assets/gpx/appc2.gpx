          <groundspeak:text encoded="False">Reasonably quick find once the GPSr settled down to the proper coordinates. TFTC</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="247516603">
          <groundspeak:date>2012-07-08T19:00:00Z</groundspeak:date>
