      <groundspeak:short_description html="True">&lt;center&gt;&lt;a href=
"http://certitude.comxa.com/certified.php?wp=GC1ZVR3"&gt;&lt;img src=
"http://certitude.comxa.com/logo.php?wp=GC1ZVR3" border=
