    <sym>Geocache</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="325116" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Tear Down This Wall!</groundspeak:name>
      <groundspeak:placed_by>MotorBug</groundspeak:placed_by>
