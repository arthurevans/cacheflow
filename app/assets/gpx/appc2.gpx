      <groundspeak:name>Edgewood</groundspeak:name>
      <groundspeak:placed_by>cmdrblop</groundspeak:placed_by>
      <groundspeak:owner id="2021846">cmdrblop</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
