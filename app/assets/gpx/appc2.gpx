          <groundspeak:text encoded="False">There weren't any donkeys around, but we found the cache.  Thanks!</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="253694990">
          <groundspeak:date>2012-07-31T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="2060522">oaknest</groundspeak:finder>
          <groundspeak:text encoded="False">Didn't see any donkeys today, but we did see all the clues and the nicely hidden replacement cache.  Thanks for keeping this well done cache alive!</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="252142826">
