          <groundspeak:finder id="1805764">camelprodder</groundspeak:finder>
          <groundspeak:text encoded="False">wat on earth were you thinking about.  Thanks for the puzzle and the hide.</groundspeak:text>
        </groundspeak:log>
        <groundspeak:log id="223037211">
          <groundspeak:date>2012-04-01T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
