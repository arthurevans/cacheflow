    <groundspeak:cache id="12971" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Fairchild Superfund</groundspeak:name>
      <groundspeak:placed_by>Otis</groundspeak:placed_by>
      <groundspeak:owner id="34854">tdchayes</groundspeak:owner>
      <groundspeak:type>Virtual Cache</groundspeak:type>
