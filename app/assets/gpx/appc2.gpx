    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="118390" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Have a Ball</groundspeak:name>
      <groundspeak:placed_by>boulter</groundspeak:placed_by>
      <groundspeak:owner id="93091">boulter</groundspeak:owner>
